		<key>PluginSettings</key>
		<dict>
			<key>ScriptInstructions</key>
			<string>Checks the S.M.A.R.T. status of all mounted disk volumes.  Ignores volumes that don't support reporting S.M.A.R.T. status.  Results in a failure if a problem is detected.</string>
			<key>ScriptResults</key>
			<dict>
				<key>0</key>
				<string>Success</string>
				<key>1</key>
				<string>The S.M.A.R.T. status indicates a disk hardware problem.</string>
			</dict>
			<key>ScriptSource</key>
			<string>-- Get the list of volumes:
do shell script "diskutil list | grep /dev/"

set volumeList to paragraphs of result
set n to count of items of volumeList
set results to 0

repeat with i from 1 to n
	set thisVolume to item i of volumeList
	try
		-- Get the SMART status of this volume:
		do shell script "diskutil info " &amp; thisVolume &amp; " | grep SMART"
		set status to result
		
		-- If the status is something other than "Not Supported" or "Verified", it's a failure:
		if (status does not end with "Not Supported") and (word 3 of status is not "Verified") then
			set results to 1
		end if
	end try
end repeat

return results
</string>
